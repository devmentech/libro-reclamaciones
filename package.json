{
  "name": "@devmentech/libro-reclamaciones",
  "version": "1.0.0",
  "description": "Componente de Libro de Reclamaciones Virtual para Perú según formato INDECOPI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup && cp src/styles/globals.css dist/styles.css",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "example:dev": "pnpm build && cd example && pnpm dev",
    "example:build": "pnpm build && cd example && pnpm build",
    "workspace:install": "pnpm install",
    "workspace:clean": "rm -rf node_modules example/node_modules && pnpm install"
  },
  "keywords": [
    "libro-reclamaciones",
    "peru",
    "indecopi",
    "nextjs",
    "react",
    "component"
  ],
  "author": "DevMenTech",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/devmentech/libro-reclamaciones.git"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@react-pdf/renderer": "^4.0.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.453.0",
    "react-day-picker": "^9.1.3",
    "react-google-recaptcha": "^3.1.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.5.0",
    "typescript": "^5"
  }
}
